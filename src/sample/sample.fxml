<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <Button text="Upload" onAction="#uploadImage"/>
    <Button GridPane.rowIndex="1" GridPane.columnIndex="0" text="Next" onAction="#nextDisplay" />
    <Button GridPane.rowIndex="1" GridPane.columnIndex="1" text="Remove" onAction="#removeIngredient" />
    <Label GridPane.rowIndex="2" fx:id="displayIngredient"/>
</GridPane>